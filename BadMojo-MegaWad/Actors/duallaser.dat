Actor DualLaser : Weapon 5666
{
	Weapon.AmmoUse 1
	Weapon.SlotNumber 4
	Weapon.AmmoGive 60
	Weapon.AmmoType "Cell"
	Inventory.PickupMessage "You picked up the Dual Laser Rifle"
	
	States
	{
	Ready:
		DUAL A 1 A_WeaponReady
		Loop
	Deselect:
		DUAL A 1 A_Lower
		Loop
	Select:
		DUAL A 1 A_Raise
		Loop
	Fire:
		DUAL A 0 A_FireCustomMissile ("D_Laser",0.5,1,4,0,0,0)
		DUAL A 0 Offset(0,39)A_FireCustomMissile("D_Laser",-0.5,1,-4,0,0,0)
		DUAL A 1 A_GunFlash
		DUAL A 0 A_PlaySound ("FIRELASERS", CHAN_WEAPON)
		DUAL AB 1
		DUAL CBA 5
		goto ready
	Flash:
		DUAL DE 1 Bright A_Light1
		goto lightdone
	Spawn:
		DUSP A -1
		stop
		
	}
}

ACTOR D_Laser
{
	Radius 13
	Height 8
	Speed 180
	Damage (5*random(4,8))
	Projectile
	+RANDOMIZE
	RenderStyle Add
	Alpha 0.75
	DeathSound "weapons/plasmax"
	Obituary "$OB_MPPLASMARIFLE"
	Decal "GatDecal"
	States
	{
	Spawn:
		LSER A 1 Bright
		Loop
	Death:
		LAZR ABCDE 4 Bright
		Stop
	}
}
